<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>dbsetup</servlet-name>
        <servlet-class>Db.dbsetup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Db.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Db.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Db.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_users</servlet-name>
        <servlet-class>Loaders.load_users</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_projects</servlet-name>
        <servlet-class>Loaders.load_projects</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_partners</servlet-name>
        <servlet-class>Loaders.load_partners</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_obligations</servlet-name>
        <servlet-class>Loaders.load_obligations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>visit_sess</servlet-name>
        <servlet-class>Loaders.visit_sess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_visits</servlet-name>
        <servlet-class>Loaders.load_visits</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>save_visit</servlet-name>
        <servlet-class>Compliance.save_visit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_visit_details</servlet-name>
        <servlet-class>Loaders.load_visit_details</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>details_sess</servlet-name>
        <servlet-class>Loaders.details_sess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dbsetup</servlet-name>
        <url-pattern>/dbsetup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_users</servlet-name>
        <url-pattern>/load_users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_projects</servlet-name>
        <url-pattern>/load_projects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_partners</servlet-name>
        <url-pattern>/load_partners</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_obligations</servlet-name>
        <url-pattern>/load_obligations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>visit_sess</servlet-name>
        <url-pattern>/visit_sess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_visits</servlet-name>
        <url-pattern>/load_visits</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save_visit</servlet-name>
        <url-pattern>/save_visit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_visit_details</servlet-name>
        <url-pattern>/load_visit_details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>details_sess</servlet-name>
        <url-pattern>/details_sess</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
